{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "vercel-node@3.0.0"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/.well-known/(.*)", "dest": "/public/.well-known/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
